    <IdentityProviderProtocol>
        <Name>SAML2 Browser Post Protocol Service Identification</Name>
        <Description>This eID IdP Protocol Service provides a SAML2 Browser POST
            Profile protocol for eID identification purposes.
        </Description>
        <ProtocolService>be.fedict.eid.idp.protocol.saml2.post.SAML2ProtocolServiceIdent</ProtocolService>
        <ContextPath>/saml2/post/ident</ContextPath>
        <Endpoints>
            <Endpoint>
                <Name>SAML2 Browser Post Identification Metadata</Name>
                <ServletClass>be.fedict.eid.idp.protocol.saml2.post.SAML2MetadataHttpServletIdent</ServletClass>
                <ContextPath>/saml2/post/metadata/ident-metadata.xml</ContextPath>
            </Endpoint>
        </Endpoints>
    </IdentityProviderProtocol>
